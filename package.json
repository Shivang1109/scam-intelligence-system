{
  "name": "scam-intelligence-system",
  "version": "1.0.0",
  "description": "AI-powered honeypot for autonomous scammer engagement and intelligence extraction",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "dev": "ts-node src/app.ts",
    "start": "node dist/app.js",
    "start:prod": "npm run build && npm start",
    "docker:build": "docker build -t scam-intel-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env scam-intel-api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f api",
    "db:init": "psql -h localhost -U scam_user -d scam_intelligence < scripts/init-db.sql",
    "db:backup": "pg_dump -h localhost -U scam_user scam_intelligence > backup-$(date +%Y%m%d-%H%M%S).sql"
  },
  "keywords": [
    "scam-detection",
    "honeypot",
    "ai",
    "intelligence",
    "fraud-detection"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "fast-check": "^3.15.1",
    "morgan": "^1.10.1",
    "pg": "^8.11.3",
    "uuid": "^13.0.0"
  }
}
